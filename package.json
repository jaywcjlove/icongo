{
  "private": true,
  "scripts": {
    "build": "lerna exec --ignore www -- npm run build",
    "watch": "lerna exec --scope svg-to-react-component -- npm run watch",
    "prepare": "husky install",
    "version": "lerna version --exact --force-publish --no-push --no-git-tag-version"
  },
  "author": "jaywcjlove",
  "license": "MIT",
  "workspaces": [
    "core",
    "icons/*",
    "www"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,json}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@types/webpack": "5.28.0",
    "husky": "8.0.1",
    "lerna": "5.4.0",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "tsbb": "3.7.6"
  }
}
