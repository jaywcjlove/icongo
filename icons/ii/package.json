{
  "name": "@icongo/ii",
  "version": "1.0.3",
  "author": "jaywcjlove",
  "description": "Iconic Icons",
  "license": "MIT",
  "homepage": "http://icongo.github.io/#/icons/ii",
  "main": "./lib/index.js",
  "module": "./esm/index.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/jaywcjlove/icongo.git"
  },
  "scripts": {
    "cpy": "cpy '../../www/public/icon/ii/*.svg' svg --flat",

    "tsbb": "tsbb build --no-source-maps",
    "icongo": "icongo --prefix II --source svg -o ./src",
    "build": "npm run cpy && npm run icongo && npm run tsbb",
    "start": "npm run build"
  },
  "keywords": [
    "icongo",
    "iconic",
    "ii",
    "react-icons",
    "icons",
    "icon"
  ]
}
