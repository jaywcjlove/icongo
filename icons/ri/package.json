{
  "name": "@icongo/ri",
  "description": "RemixIcon Icons",
  "version": "1.0.3",
  "main": "./lib/index.js",
  "module": "./esm/index.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "homepage": "http://icongo.github.io/#/icons/ri",
  "scripts": {
    "cpy": "cpy '../../www/public/icon/ri/*.svg' svg --flat",

    "icongo": "icongo --prefix RI --source svg -o ./src -r='{\"bookmark-fill\": \"RIBookmarkFill1\", \"bookmark-line\": \"RIBookmarkLine1\"}'",
    "build": "npm run cpy && npm run icongo && tsbb build --no-source-maps",
    "start": "npm run build"
  },
  "keywords": [
    "icongo",
    "RemixIcon",
    "ri",
    "go",
    "react-icons",
    "icons",
    "icon"
  ],
  "author": "jaywcjlove",
  "license": "MIT"
}
